<Application x:Class="GigaHunt.App" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:local="clr-namespace:GigaHunt" >
  <Application.Resources>
    <ResourceDictionary >
      <ResourceDictionary.MergedDictionaries>
        <!--fallback theme + designer's:-->
        <ResourceDictionary Source="/WpfUserControlLib;component/Base/BasicDefaultsLayout.xaml" />
        <ResourceDictionary Source="/WpfUserControlLib;component/ColorScheme/Theme.Color.Dark.Google.xaml" />
        <!--
        <ResourceDictionary Source="/WpfUserControlLib;component/Base/DataGrid.Base.xaml" />
        Allows Style="{StaticResource WindowStyle_Aav0}"  (since it is on top of the visual tree over the Window) -->
      </ResourceDictionary.MergedDictionaries>
      <SolidColorBrush x:Key="AgntButtonForeground" Color="#aa0"/>
      <SolidColorBrush x:Key="LeadButtonForeground" Color="#06f"/>
      <SolidColorBrush x:Key="EmlrButtonForeground" Color="#e08"/>
      <SolidColorBrush x:Key="OuDbButtonForeground" Color="#0a0"/>
    </ResourceDictionary >
  </Application.Resources>
</Application>
